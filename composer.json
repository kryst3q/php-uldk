{
    "name": "krystian/php-uldk",
    "description": "Object oriented library written in php for obtaining polish cadastral plot geographic localisation using it's number or coordinates. The library uses ULDK www service for obtaining informations (http://uldk.gugik.gov.pl/opis.html).",
    "type": "library",
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "symplify/easy-coding-standard": "^7.2",
        "sylius-labs/coding-standard": "^3.1",
        "symfony/var-dumper": "^5.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Krystian Kulesz",
            "email": "krystian.kulesz@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "7.4.*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Kryst3q\\PhpUldk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kryst3q\\PhpUldk\\Tests\\": "tests/"
        }
    }
}
